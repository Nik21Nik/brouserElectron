{
  "name": "my-browser-app",
  "version": "1.0.0",
  "description": "Минималистичный браузер на Electron",
  "main": "main.js",
  "author": "Ваше Имя",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.mycompany.mybrowser",
    "productName": "MyBrowser",
    "files": [
      "**/*",
      "!node_modules/**/*dev.js",
      "!**/*.md"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico"
    }
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.34.0",
    "cross-fetch": "^4.1.0",
    "node-fetch": "^3.3.2",
    "punycode": "^2.3.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^24.0.0"
  }
}
